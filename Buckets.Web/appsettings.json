{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*",
    "UseForwardedHeaders": false,
    "UseHttpsRedirection": true,
    "EnableSwaggerDocumentation": false,
    "BucketPath": "./buckets",
    "AuthenticationRequirements": {
        "BucketList": true,
        "ObjectList": true,
        "ObjectRead": true,
        "ObjectCreate": true,
        "ObjectDelete": true
    },
    "AuthenticationKeys": [
        "CHANGE_ME"
    ]
}
